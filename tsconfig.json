{
    "compilerOptions": {
        "target": "es5",
        "module": "CommonJS",
        "lib": ["ES2022", "dom", "ES2019", "ES7"],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "types": [
            "cypress",
            "node"
        ]
    },
    "include": ["**/*.ts", "cypress/support/commands.js"]
}
